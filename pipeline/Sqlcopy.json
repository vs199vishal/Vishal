{
	"name": "Sqlcopy",
	"properties": {
		"description": "Copy transactions ",
		"activities": [
			{
				"name": "Copy_jug",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "vishal/input/transactions.txt"
					},
					{
						"name": "Destination",
						"value": "[dbo].[transactions]"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"tdate": "tdate",
							"amount": "amount"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_jug",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_jug",
						"type": "DatasetReference"
					}
				]
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}